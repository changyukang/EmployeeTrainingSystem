<% provide(:title, @group.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @group.name %>
      </h1>
    </section>
  </aside>
</div>



<p id="notice"><%= notice %></p>

<% @members.each do |member| %>
    <ul>
      <td><%= link_to member.name, member %></td>
    </ul>
<% end %>

<%= link_to 'Edit', edit_group_path(@group) %> |
<%= link_to 'Back', groups_path %>