<% provide(:title, "Employee Status") %>

<h1>Employee Progress</h1>

<p>
  <ul class="users">
    <% @user_courses.each do |uc| %>
    <li>
    <%= User.where(:id => uc.user_id).pluck(:name).first %> | 
    <%= Course.where(:id => uc.course_id).pluck(:name).first %> | 
    <%= uc.progress*100 %>%
    </li>
    <% end %>  
</ul>
</p>
