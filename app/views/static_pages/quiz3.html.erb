<% provide(:title, 'Quiz 1') %>
<h1>Quiz 1</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@score) do |f| %>
      <%= render 'shared/error_messages', obj: @score %>

      <%= f.label :a %>
      <%= f.radio_button :a, 'Question 1',class: 'form-control' %>

      <%= f.submit "Submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>